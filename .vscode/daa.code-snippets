            "    cin >> text;",
            "    cin >> pattern;",
            "",
            "    int n = text.length();",
            "    int m = pattern.length();",
            "    bool found = false;",
            "",
            "    for (int i = 0; i <= n - m; i++) {",
            "        int j = 0;",
            "",
            "        while (j < m && text[i + j] == pattern[j]) {",
            "            j++;",
            "        }",
            "",
            "        if (j == m) {",
            "            cout << \"Pattern found at index \" << i << endl;",
            "            found = true;",
            "        }",
            "    }",
            "",
            "    if (found == false) {",
            "        cout << \"Pattern not found.\" << endl;",
            "    }",
            "",
            "    return 0;",
            "}"
