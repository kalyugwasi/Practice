    "Max Min": {
        "scope": "c,cpp",
        "prefix": "maxmin",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    int n;",
            "",
            "    cout << \"Enter number of elements: \";",
            "    cin >> n;",
            "",
            "    int arr[n];",
            "",
            "    cout << \"Enter \" << n << \" elements: \";",
            "    for (int i = 0; i < n; i++) {",
            "        cin >> arr[i];",
            "    }",
            "",
            "    int low = 0;",
            "    int high = n - 1;",
            "    int mid, minVal, maxVal;",
            "",
            "    int leftMin, leftMax, rightMin, rightMax;",
            "",
            "    mid = (low + high) / 2;",
            "",
            "    leftMin = leftMax = arr[low];",
            "    for (int i = low; i <= mid; i++) {",
            "        if (arr[i] < leftMin) {",
            "            leftMin = arr[i];",
            "        }",
            "        if (arr[i] > leftMax) {",
            "            leftMax = arr[i];",
            "        }",
            "    }",
            "",
            "    rightMin = rightMax = arr[mid + 1];",
            "    for (int i = mid + 1; i <= high; i++) {",
            "        if (arr[i] < rightMin) {",
            "            rightMin = arr[i];",
            "        }",
            "        if (arr[i] > rightMax) {",
            "            rightMax = arr[i];",
            "        }",
            "    }",
            "",
            "    if (leftMin < rightMin) {",
            "        minVal = leftMin;",
            "    } else {",
            "        minVal = rightMin;",
            "    }",
            "",
            "    if (leftMax > rightMax) {",
            "        maxVal = leftMax;",
            "    } else {",
            "        maxVal = rightMax;",
            "    }",
            "",
            "    cout << \"Minimum element: \" << minVal << endl;",
            "    cout << \"Maximum element: \" << maxVal << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Find maximum and minimum element"
    },
    "Fractional Knapsack": {
        "scope": "c,cpp",
        "prefix": "fractionalknapsack",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "struct Item {",
            "    int value;",
            "    int weight;",
            "};",
            "",
            "int main() {",
            "    int n, capacity;",
            "",
            "    cout << \"Enter number of items and knapsack capacity: \";",
            "    cin >> n >> capacity;",
            "",
            "    Item items[n];",
            "",
            "    cout << \"Enter value and weight of each item:\\n\";",
            "    for (int i = 0; i < n; i++) {",
            "        cin >> items[i].value >> items[i].weight;",
            "    }",
            "",
            "    for (int i = 0; i < n - 1; i++) {",
            "        for (int j = i + 1; j < n; j++) {",
            "            double r1 = (double)items[i].value / items[i].weight;",
            "            double r2 = (double)items[j].value / items[j].weight;",
            "",
            "            if (r1 < r2) {",
            "                Item temp = items[i];",
            "                items[i] = items[j];",
            "                items[j] = temp;",
            "            }",
            "        }",
            "    }",
            "",
            "    double totalValue = 0.0;",
            "",
            "    for (int i = 0; i < n; i++) {",
            "        if (capacity == 0) {",
            "            break;",
            "        }",
            "",
            "        if (items[i].weight <= capacity) {",
            "            capacity -= items[i].weight;",
            "            totalValue += items[i].value;",
            "        } else {",
            "            double fraction = (double)capacity / items[i].weight;",
            "            totalValue += items[i].value * fraction;",
            "            capacity = 0;",
            "        }",
            "    }",
            "",
            "    cout << \"Maximum value in knapsack = \" << totalValue;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Fractional Knapsack algorithm"
    },
    "4 Queens": {
        "scope": "c,cpp",
        "prefix": "4queens",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int board[4];",
            "",
            "bool safe(int r, int c) {",
            "    for (int i = 0; i < c; i++) {",
            "        if (board[i] == r || abs(board[i] - r) == abs(i - c)) {",
            "            return false;",
            "        }",
            "    }",
            "    return true;",
            "}",
            "",
            "void solve(int c) {",
            "    if (c == 4) {",
            "        for (int i = 0; i < 4; i++) {",
            "            for (int j = 0; j < 4; j++) {",
            "                if (board[j] == i) {",
            "                    cout << \"Q \";",
            "                } else {",
            "                    cout << \". \";",
            "                }",
            "            }",
            "            cout << endl;",
            "        }",
            "        cout << endl;",
            "        return;",
            "    }",
            "",
            "    for (int r = 0; r < 4; r++) {",
            "        if (safe(r, c)) {",
            "            board[c] = r;",
            "            solve(c + 1);",
            "        }",
            "    }",
            "}",
            "",
            "int main() {",
            "    solve(0);",
            "    return 0;",
            "}"
        ],
        "description": "4 Queens problem"
    },
    "Matrix Chain Multiplication": {
        "scope": "c,cpp",
        "prefix": "matrixchain",
        "body": [
            "#include <iostream>",
            "#include <climits>",
            "using namespace std;",
            "",
            "int matrixChainMultiplication(int arr[], int n) {",
            "    int dp[n][n];",
            "",
            "    for (int i = 1; i < n; i++) {",
            "        dp[i][i] = 0;",
            "    }",
            "",
            "    for (int l = 2; l < n; l++) {",
            "        for (int i = 1; i < n - l + 1; i++) {",
            "            int j = i + l - 1;",
            "            dp[i][j] = INT_MAX;",
            "",
            "            for (int k = i; k < j; k++) {",
            "                int cost = dp[i][k] + dp[k + 1][j] + arr[i - 1] * arr[k] * arr[j];",
            "",
            "                if (cost < dp[i][j]) {",
            "                    dp[i][j] = cost;",
            "                }",
            "            }",
            "        }",
            "    }",
            "",
            "    return dp[1][n - 1];",
            "}",
            "",
            "int main() {",
            "    int n;",
            "",
            "    cout << \"Enter number of matrices: \";",
            "    cin >> n;",
            "",
            "    int arr[n + 1];",
            "",
            "    cout << \"Enter dimensions: \";",
            "    for (int i = 0; i < n + 1; i++) {",
            "        cin >> arr[i];",
            "    }",
            "",
            "    cout << \"Minimum number of multiplications: \"",
            "         << matrixChainMultiplication(arr, n + 1);",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Matrix Chain Multiplication algorithm"
    },
    "Boyer Moore": {
        "scope": "c,cpp",
        "prefix": "boyermoore",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    string text, pattern;",
            "",
            "    cout << \"Enter Text: \";",
            "    cin >> text;",
            "    cout << \"Enter Pattern: \";",
            "    cin >> pattern;",
            "",
            "    int n = text.length();",
            "    int m = pattern.length();",
            "",
            "    int badChar[256];",
            "",
            "    for (int i = 0; i < 256; i++) {",
            "        badChar[i] = -1;",
            "    }",
            "",
            "    for (int i = 0; i < m; i++) {",
            "        badChar[(int)pattern[i]] = i;",
            "    }",
            "",
            "    int shift = 0;",
            "",
            "    while (shift <= n - m) {",
            "        int j = m - 1;",
            "",
            "        while (j >= 0 && pattern[j] == text[shift + j]) {",
            "            j--;",
            "        }",
            "",
            "        if (j < 0) {",
            "            cout << \"Pattern found at index \" << shift << endl;",
            "            if (shift + m < n) {",
            "                shift += m - badChar[text[shift + m]];",
            "            } else {",
            "                shift += 1;",
            "            }",
            "        } else {",
            "            shift += max(1, j - badChar[text[shift + j]]);",
            "        }",
            "    }",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Boyer-Moore string matching algorithm"
    },
    "0/1 Knapsack": {
        "scope": "c,cpp",
        "prefix": "01knapsack",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int knapsack(int w, int wt[], int val[], int n) {",
            "    if (n == 0) {",
            "        return 0;",
            "    }",
            "    if (w == 0) {",
            "        return 0;",
            "    }",
            "    if (wt[n - 1] > w) {",
            "        return knapsack(w, wt, val, n - 1);",
            "    } else {",
            "        return max(",
            "            val[n - 1] + knapsack(w - wt[n - 1], wt, val, n - 1),",
            "            knapsack(w, wt, val, n - 1)",
            "        );",
            "    }",
            "}",
            "",
            "int main() {",
            "    int n;",
            "",
            "    cout << \"Enter number of items: \";",
            "    cin >> n;",
            "",
            "    int wt[n];",
            "    int val[n];",
            "",
            "    for (int i = 0; i < n; i++) {",
            "        cin >> wt[i];",
            "    }",
            "",
            "    for (int i = 0; i < n; i++) {",
            "        cin >> val[i];",
            "    }",
            "",
            "    int w;",
            "    cout << \"Enter capacity of knapsack: \";",
            "    cin >> w;",
            "",
            "    cout << \"Maximum value in knapsack = \"",
            "         << knapsack(w, wt, val, n) << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "0/1 Knapsack problem"
    },
