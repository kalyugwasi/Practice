    "Set Cover Problem": {
        "scope": "c,cpp",
        "prefix": "setcover",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    int universalSet[100];",
            "    int sets[100][100];",
            "    int n, m[100];",
            "    int k;",
            "",
            "    cout << \"Enter the number of elements in the universal set: \";",
            "    cin >> n;",
            "",
            "    cout << \"Enter the elements of the universal set: \";",
            "    for (int i = 0; i < n; i++) {",
            "        cin >> universalSet[i];",
            "    }",
            "",
            "    cout << \"Enter the number of subsets: \";",
            "    cin >> k;",
            "",
            "    for (int i = 0; i < k; i++) {",
            "        cout << \"Enter the number of elements in subset \" << i + 1 << \": \";",
            "        cin >> m[i];",
            "",
            "        cout << \"Enter the elements of subset \" << i + 1 << \": \";",
            "        for (int j = 0; j < m[i]; j++) {",
            "            cin >> sets[i][j];",
            "        }",
            "    }",
            "",
            "    bool covered[100];",
            "    for (int i = 0; i < n; i++) {",
            "        covered[i] = false;",
            "    }",
            "",
            "    int selectedSets[100];",
            "    int selectedCount = 0;",
            "",
            "    while (true) {",
            "        int maxCover = 0;",
            "        int bestSet = -1;",
            "",
            "        for (int i = 0; i < k; i++) {",
            "            int count = 0;",
            "",
            "            for (int j = 0; j < m[i]; j++) {",
            "                for (int l = 0; l < n; l++) {",
            "                    if (sets[i][j] == universalSet[l] && covered[l] == false) {",
            "                        count++;",
            "                    }",
            "                }",
            "            }",
            "",
            "            if (count > maxCover) {",
            "                maxCover = count;",
            "                bestSet = i;",
            "            }",
            "        }",
            "",
            "        if (bestSet == -1) {",
            "            break;",
            "        }",
            "",
            "        selectedSets[selectedCount] = bestSet;",
            "        selectedCount++;",
            "",
            "        for (int j = 0; j < m[bestSet]; j++) {",
            "            for (int l = 0; l < n; l++) {",
            "                if (sets[bestSet][j] == universalSet[l]) {",
            "                    covered[l] = true;",
            "                }",
            "            }",
            "        }",
            "    }",
            "",
            "    cout << \"Selected subsets for the set cover are: \";",
            "    for (int i = 0; i < selectedCount; i++) {",
            "        cout << selectedSets[i] + 1 << \" \";",
            "    }",
            "    cout << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Set Cover Problem algorithm"
    },
    "Vertex Cover": {
        "scope": "c,cpp",
        "prefix": "vertexcover",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    int n, e;",
            "    cout << \"Enter the number of vertices: \";",
            "    cin >> n;",
            "",
            "    cout << \"Enter the number of edges: \";",
            "    cin >> e;",
            "",
            "    int edges[100][2];",
            "",
            "    cout << \"Enter the edges (vertex1 vertex2):\" << endl;",
            "    for (int i = 0; i < e; i++) {",
            "        cin >> edges[i][0] >> edges[i][1];",
            "    }",
            "",
            "    bool visited[100];",
            "    for (int i = 0; i < n; i++) {",
            "        visited[i] = false;",
            "    }",
            "",
            "    for (int i = 0; i < e; i++) {",
            "        int u = edges[i][0];",
            "        int v = edges[i][1];",
            "",
            "        if (visited[u] == false && visited[v] == false) {",
            "            visited[u] = true;",
            "            visited[v] = true;",
            "        }",
            "    }",
            "",
            "    cout << \"Vertices in the Vertex Cover are: \";",
            "    for (int i = 0; i < n; i++) {",
            "        if (visited[i] == true) {",
            "            cout << i << \" \";",
            "        }",
            "    }",
            "    cout << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Vertex Cover algorithm"
    },
    "BFS": {
        "scope": "c,cpp",
        "prefix": "bfs",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    int n;",
            "    cout << \"Enter the number of vertices: \";",
            "    cin >> n;",
            "",
            "    int adj[100][100];",
            "    cout << \"Enter the adjacency matrix:\" << endl;",
            "    for (int i = 0; i < n; i++) {",
            "        for (int j = 0; j < n; j++) {",
            "            cin >> adj[i][j];",
            "        }",
            "    }",
            "",
            "    int start;",
            "    cout << \"Enter the starting vertex: \";",
            "    cin >> start;",
            "",
            "    bool visited[100];",
            "    for (int i = 0; i < n; i++) {",
            "        visited[i] = false;",
            "    }",
            "",
            "    int queue[100];",
            "    int front = 0, rear = 0;",
            "",
            "    queue[rear] = start;",
            "    rear++;",
            "    visited[start] = true;",
            "",
            "    cout << \"BFS Traversal: \";",
            "",
            "    while (front < rear) {",
            "        int current = queue[front];",
            "        front++;",
            "",
            "        cout << current << \" \";",
            "",
            "        for (int i = 0; i < n; i++) {",
            "            if (adj[current][i] == 1 && visited[i] == false) {",
            "                queue[rear] = i;",
            "                rear++;",
            "                visited[i] = true;",
            "            }",
            "        }",
            "    }",
            "",
            "    cout << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Breadth-First Search (BFS)"
    },
    "DFS": {
        "scope": "c,cpp",
        "prefix": "dfs",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int n;",
            "int adj[100][100];",
            "bool visited[100];",
            "",
            "void DFS(int v) {",
            "    visited[v] = true;",
            "    cout << v << \" \";",
            "",
            "    for (int i = 0; i < n; i++) {",
            "        if (adj[v][i] == 1 && visited[i] == false) {",
            "            DFS(i);",
            "        }",
            "    }",
            "}",
            "",
            "int main() {",
            "    cout << \"Enter the number of vertices: \";",
            "    cin >> n;",
            "",
            "    cout << \"Enter the adjacency matrix:\" << endl;",
            "    for (int i = 0; i < n; i++) {",
            "        for (int j = 0; j < n; j++) {",
            "            cin >> adj[i][j];",
            "        }",
            "    }",
            "",
            "    for (int i = 0; i < n; i++) {",
            "        visited[i] = false;",
            "    }",
            "",
            "    int start;",
            "    cout << \"Enter the starting vertex: \";",
            "    cin >> start;",
            "",
            "    cout << \"DFS Traversal: \";",
            "    DFS(start);",
            "    cout << endl;",
            "",
            "    return 0;",
            "}"
        ],
        "description": "Depth-First Search (DFS)"
