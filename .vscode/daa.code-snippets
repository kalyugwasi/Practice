    "Bubble Sort": {
        "scope": "c,cpp",
        "prefix": "bubblesort",
        "body": [
            "#include <iostream>",
            "using namespace std;",
            "",
            "int main() {",
            "    int n;",
            "    cout << \"Enter number of elements: \";",
            "    cin >> n;",
            "",
            "    int arr[n];",
            "    cout << \"Enter \" << n << \" numbers: \";",
            "    for (int i = 0; i < n; i++){",
            "        cin >> arr[i];",
            "    }",
            "",
            "    for (int i = 0; i < n - 1; i++){",
            "        for (int j = 0; j < n - i - 1; j++){",
            "            if (arr[j] > arr[j + 1]){",
            "                swap(arr[j], arr[j + 1]);",
            "            }",
            "        }",
            "    }",
            "",
            "    cout << \"Sorted numbers: \";",
            "    for (int i = 0; i < n; i++){",
            "        cout << arr[i]<<\" \";",
            "    }",
            "}"
        ],
        "description": "Bubble Sort algorithm"
